{
  "name": "porybox",
  "private": true,
  "version": "0.0.1",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "angular": "1.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "bluebird": "3.3.0",
    "browserify": "13.0.0",
    "concurrently": "^1.0.0",
    "ejs": "2.3.4",
    "grunt": "0.4.5",
    "include-all": "0.1.6",
    "rc": "1.0.1",
    "sails": "0.12.0",
    "sails-disk": "0.10.9",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "bundle": "browserify client/app.js -o client/bundle.js -t babelify -d",
    "watchbundle": "watchify client/app.js -o client/bundle.js -t babelify -d -v",
    "dev": "concurrent 'npm run testwatch' 'npm run watchbundle' 'node app.js'",
    "start": "npm run bundle && node app.js",
    "test": "mocha && karma start --single-run",
		"testwatch": "concurrent 'mocha -w' 'karma start'"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:porygonco/porybox.git"
  },
  "author": "PorygonCo",
  "license": "",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  }
}
