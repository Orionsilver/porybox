{
  "name": "porybox",
  "private": true,
  "version": "0.0.1",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "angular": "1.5.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "bcryptjs": "^2.3.0",
    "bluebird": "3.3.0",
    "browserify": "13.0.0",
    "concurrently": "^1.0.0",
    "connect-mongo": "^0.8.2",
    "ejs": "2.3.4",
    "eslint": "^2.0.0",
    "grunt": "0.4.5",
    "include-all": "0.1.6",
    "less": "^2.6.0",
    "material-design-lite": "^1.1.1",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "rc": "1.0.1",
    "sails": "0.12.0",
    "sails-disk": "0.10.9",
    "sails-mongo": "^0.12.0",
    "validator": "^4.8.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "bundle": "lessc client/styles/importer.less client/bundle.css && browserify client/app.js -o client/bundle.js -t babelify -d",
    "watchbundle": "watchify client/app.js -o client/bundle.js -t babelify -d -v",
    "dev": "concurrent 'npm run watchbundle' 'node app.js'",
    "lint": "eslint ./",
    "start": "npm run bundle && node app.js",
    "test": "npm run lint && mocha --compilers js:babel-core/register && karma start --single-run",
    "testwatch": "concurrent 'mocha -w --compilers js:babel-core/register' 'karma start'"
  },
  "main": "app.js",
